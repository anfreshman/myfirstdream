//两个数值相比较
#include <stdio.h>
int main ( )
{int max(int a,int b);
int a,b,c;
scanf("%d,%d",&a,&b);
c=max (a,b);
printf("最大的是%d\n",c);
return 0;}
int max(int x,int y)
{int z;
if(x>y)z=x;
else z=y;
return (z);}






